{
  "extends": ["next/core-web-vitals", "eslint:recommended"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    // Your specific rules.
    "no-unused-vars": "warn",
    "no-mixed-spaces-and-tabs": 0,
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off",
    "indent": ["error", "tab"],
    "linebreak-style": "off",
    "quotes": ["error", "single"],
    "semi": ["error", "always"]
  }
}
